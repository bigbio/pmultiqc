# Example deployment configuration for production
# This example demonstrates the solution for https://abi-services.cs.uni-tuebingen.de/pmultiqc/
# This demonstrates the solution to the BASE_URL configuration issue

BASE_URL=https://abi-services.cs.uni-tuebingen.de/pmultiqc
HOST_PORT=8080
FLASK_ENV=production
MAX_CONTENT_LENGTH=4294967296
UPLOAD_FOLDER=/tmp/pmultiqc_uploads
OUTPUT_FOLDER=/tmp/pmultiqc_outputs
HTML_REPORTS_FOLDER=/tmp/pmultiqc_html_reports
LOG_LEVEL=INFO
REDIS_URL=redis://redis:6379
REDIS_USERNAME=
REDIS_PASSWORD=
REDIS_DB=0
MEMORY_LIMIT=16384M
CPU_LIMIT=4.0
MEMORY_RESERVATION=12288M
CPU_RESERVATION=1.0